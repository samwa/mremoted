<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.runtime.remoting>
		<application>
			<service>
				<wellknown
					mode="Singleton"
					type="Samwa.RemoteClass, RemoteClass"
					objectUri="RemoteClass"
				/>
			</service>
			<channels>
				<channel ref="tcp" port="8888">
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
					</serverProviders>
				</channel>
			</channels>
		</application>
	</system.runtime.remoting>
</configuration>